#content a,.nav-link{color:#ff10f0;text-decoration:none}#content a *,.nav-link *{color:#ff10f0}#content a:hover,.nav-link:hover{color:#ff10f0;text-decoration:underline}#footer a,.navbar-brand{color:#ff10f0;text-decoration:none}#footer a *,.navbar-brand *{color:#ff10f0}#footer a:hover,.navbar-brand:hover{color:#ff10f0;text-decoration:underline}#content table td,#content table th{border:1px solid #ff10f0;padding:6px 12px;text-align:left}*{color:#ff10f0;font-family:inconsolata;line-height:1.2}html,body{background-color:#000}.container{max-width:750px}.navbar-brand{font-size:2rem}.navbar-brand.ascii-art-title pre{color:#fff}#content p{margin-bottom:1.2rem}#content h1,#content h2,#content h3,#content h4,#content h5,#content h6{font-size:medium;font-weight:700;margin:1.2rem 0 .6rem;color:#ff69ff}#content h1{font-size:1.8rem}#content h2{font-size:1.6rem}#content h3{font-size:1.4rem}#content h4{font-size:1.2rem}#content img{display:block;margin:1.2rem auto}#content .avatar>img{border-radius:50%;float:right;margin:-8px 0 0 16px;height:150px;width:150px}#content .cover>img{float:left;margin:16px;object-fit:contain;height:150px;width:150px}#content .book{margin:16px;height:auto}#content ol{counter-reset:list;list-style:none;padding-left:0}#content ol>li:before{content:"[" counter(list,decimal)"] ";counter-increment:list}#content ul{list-style:inside;padding-left:0}#content table{margin:1.2rem auto;width:100%}#content table th{font-weight:700}#content table tr:nth-child(2n){background-color:#1a1a1a}#content blockquote{border-left:4px solid #ff10f0;font-style:italic;margin:1.2rem 0;padding:8px}#content code{color:#ff10f0;background-color:#1a1a1a;border:1px solid #ff10f0;border-radius:10%;padding:0 4px}#content pre code{all:unset}#content .highlight{margin:1.2rem auto}#content .highlight>pre{padding:8px}